node {
    def app
        stage('clone') {
            checkout scm
        }
        stage('build image') {
            app = docker.build('nginx:latest')
        }
        stage('run image') {
            docker.image('nginx:latest').withRun('-p 80:80') { c -> 
                sh 'docker ps'
                sh 'curl localhost:80'
            }
        }
}
